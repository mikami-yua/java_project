package day11;

public class Chinese {
	
	String id;
	String name;
	//国籍一定是中国，和上面的实例变量不同
	//String country;浪费内存
	//国籍属于类级别的特征，可以提升为整个模板的特征，在变量前加static关键字
	//静态变量在类加载时就初始化，不需要创建对象内存就开辟了
	//静态变量存储在方法区内存中
	static String country;
	
	public Chinese() {
		
	}
	public Chinese(String id, String name, String country) {
		super();
		this.id = id;
		this.name = name;
		this.country = country;
	}
		
		
}
